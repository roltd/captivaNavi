path = "\Storage Card\scripts\"
If("PC" eq MortScriptType())
	path = "f:\"
EndIf

path = path & "TurboDogMain.mscr"
if( FileExists( path ))
	CallScript(path)
else
	path = SystemPath("ScriptPath") \ "SystemSet_orig.in_"
	port = IniRead(path, "GPS", "ComPort")
	path = SystemPath("ScriptPath") \ "SystemSet.in_"
	IniWrite( path, "GPS", "ComPort", port )
EndIf
RunWait(SystemPath("ScriptPath") \"TurboDog_orig.exe") 
